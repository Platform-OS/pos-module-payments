{% parse_json object %}
  {
    "id": {{ gateway_request.id | json }},
    "response_body": {{ object.response.body | json }},
    "response_status": "{{ object.response.status }}",
    "gateway_object_id": {{ object.response.body | parse_json | dig: 'id' | json }}
  }
{% endparse_json %}

{% liquid
  return object
%}
