{% liquid
  if profile_id == blank
    log 'Something went wrong. Profile cannot be blank.', type: 'ERROR'
    return null
  endif

  graphql r  = 'transactions/search', limit: 1, profile_id: profile_id, id: id
  include 'lib/handle_graphql_response', r: r, name: 'queries/transactions/find_by_profile'

  return r.transactions
%}
