{% liquid
  assign data = '{ "foo": "bar" }' | parse_json
  assign url = "https://{{ context.location.host }}/payments/webhook"
  function object = 'modules/payments/commands/gateway_requests/receive', object: data, name: 'webhook_payment_finished', external_id: "5", request_url: url

  include 'test/assertions/valid_object', contract: contract, object: object, field_name: 'base'
  assign payload = object.request_data | parse_json
  include 'test/assertions/equal', contract: contract, given: payload, field_name: 'object', expected: data
  include 'test/assertions/equal', contract: contract, given: object.response_status, field_name: 'response_status', expected: null
%}
