{% parse_json data %}
{
  "to": "https://{{ context.location.host }}/not_exists",
  "request_type": "GET",
  "payload": { "foo": "bar" }
}
{% endparse_json %}
{% liquid
  function object = 'modules/payments/commands/gateway_requests/send', object: data, template: 'generic', name: 'checkout_request,' external_id: "5"

  include 'test/assertions/valid_object', contract: contract, object: object, field_name: 'base'
  assign payload = object.request_data | parse_json
  include 'test/assertions/equal', contract: contract, given: payload, field_name: 'object', expected: data
  include 'test/assertions/equal', contract: contract, given: object.response_status, field_name: 'response_status', expected: "404"
%}
